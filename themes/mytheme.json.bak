	"tokenColors": [
		{
			"name": "Comments",
			"scope": "comment, punctuation.definition.comment",
			"settings": {
				"foreground": "#2FCB71",
				"fontStyle": ""
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#72a9e0"
			}
		},
		{
			"name": "Markup Quote",
			"scope": "markup.quote, punctuation.definition.string.begin, punctuation.definition.string.end",
			"settings": {
				"foreground": "#72a9e0"
			}
		},
		{
			"name": "Variable php",
			"scope": "variable",
			"settings": {
				"foreground": "#BEBEBE"
			}
		},
		{
			"name": "Variable php",
			"scope": "punctuation.definition.variable.php",
			"settings": {
				"foreground": "#66D9EE"
			}
		},
		{
			"name": "punctuation section embedded php, doctype html",
			"scope": "punctuation.section.embedded.end.php, meta.embedded.block.php, punctuation.section.embedded.begin.php, meta.tag.sgml.doctype.html, meta.tag.sgml.html",
			"settings": {
				"foreground": "#f99157"
			}
		},
		{
			"name": "Integers",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#A5C261"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": "keyword, storage.type, storage.modifier",
			"settings": {
				"foreground": "#66D9EE"
			}
		},
		{
			"name": "Tag",
			"scope": "entity.name.tag, meta.tag.sgml, markup.deleted.git_gutter",
			"settings": {
				"foreground": "#fac863"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": "punctuation.definition.tag, punctuation.definition.tag.html, punctuation.definition.tag.begin.html, punctuation.definition.tag.end.html, punctuation.definition.tag.xml, punctuation.definition.tag.begin.xml, punctuation.definition.tag.end.xml",
			"settings": {
				"foreground": "#fac863"
			}
		},
		{
			"name": "Attributes",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#A5C261"
			}
		},
		{
			"name": "Tag css",
			"scope": "entity.name.tag.css",
			"settings": {
				"foreground": "#A5C261"
			}
		},
		{
			"name": "Class, Support",
			"scope": "entity.name.class, entity.name.type.class, support.type, support.class, support.orther.namespace.use.php, meta.use.php, support.other.namespace.php, markup.changed.git_gutter",
			"settings": {
				"foreground": "#FAC863"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": "constant.other.color, keyword.other.template, keyword.other.substitution",
			"settings": {
				"foreground": "#5FB3B3"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#BEBEA1"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": "punctuation, punctuation.definition.tag, punctuation.separator.inheritance.php, punctuation.section.embedded",
			"settings": {
				"foreground": "#BEBEA1"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": "constant.language, constant.character, variable.parameter",
			"settings": {
				"foreground": "#f99157"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": "constant.numeric.css, support.constant, keyword.other.unit",
			"settings": {
				"foreground": "#5fb3b3"
			}
		},
		{
			"name": "Function, Special Method, Block Level",
			"scope": "entity.name.function, variable.function, support.function, keyword.other.special-method, meta.block-level",
			"settings": {
				"foreground": "#f1473b"
			}
		}
	]
}